<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cofii.ts.first.VFDController">
   <children>
      <HBox spacing="6.0">
         <children>
            <TextField fx:id="tfText" promptText="Text" />
            <TextField fx:id="tfColumn" promptText="Column" />
            <Button fx:id="btnFind" mnemonicParsing="false" text="Find" />
         </children>
         <padding>
            <Insets bottom="6.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox fx:id="hbSearch">
         <children>
            <Label fx:id="lbSearch" text="Search Result: " />
            <Label fx:id="lbResult" text="0" />
         </children>
         <padding>
            <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
         </padding>
      </HBox>
   </children>
</VBox>
